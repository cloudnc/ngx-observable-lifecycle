{
  "name": "ngx-observable-lifecycle",
  "version": "0.0.0-development",
  "scripts": {
    "------------------ CORE COMMANDS -----------------": "",
    "ng": "ng",
    "postinstall": "ngcc",
    "------------------ LINT COMMANDS -----------------": "",
    "prettier": "prettier",
    "prettier:base": "yarn run prettier \"**/*.{js,json,scss,md,ts,html}\"",
    "prettier:write": "yarn run prettier:base --write",
    "prettier:check": "yarn run prettier:base --list-different",
    "------------------ DEMO COMMANDS -----------------": "",
    "demo:start": "yarn run ng serve",
    "demo:build:base": "yarn run ng build",
    "demo:build:prod": "yarn run demo:build:base --configuration production",
    "demo:test": "yarn run ng test",
    "demo:lint:check": "yarn run ng lint",
    "demo:lint:fix": "yarn run demo:lint:check --fix",
    "------------------ LIB COMMANDS -----------------": "",
    "lib:build:prod": "yarn run ng build --project ngx-observable-lifecycle --configuration production",
    "lib:build:watch": "yarn run lib:build:prod --watch",
    "lib:test:watch": "yarn run ng test --project ngx-observable-lifecycle --code-coverage",
    "lib:test:ci": "yarn run ng test --project ngx-observable-lifecycle --watch false --browsers ChromeHeadless --code-coverage",
    "------------------ RELEASE COMMANDS ------------------": "",
    "semantic-release": "semantic-release",
    "readme:build": "embedme README.md",
    "readme:check": "embedme README.md --verify",
    "------------------ QUICK COMMANDS ------------------": "",
    "lint:fix": "yarn demo:lint:fix && yarn prettier:write",
    "test": "yarn lib:test:watch",
    "start": "yarn demo:start",
    "commit": "git add . && git-cz"
  },
  "dependencies": {
    "@angular/animations": "~13.0.2",
    "@angular/common": "~13.0.2",
    "@angular/compiler": "~13.0.2",
    "@angular/core": "~13.0.2",
    "@angular/forms": "~13.0.2",
    "@angular/platform-browser": "~13.0.2",
    "@angular/platform-browser-dynamic": "~13.0.2",
    "@angular/router": "~13.0.2",
    "rxjs": "~6.5.4",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.0.3",
    "@angular/cli": "~13.0.3",
    "@angular/compiler-cli": "~13.0.2",
    "@angular/language-service": "~13.0.2",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codecov": "^3.7.0",
    "codelyzer": "^6.0.0",
    "commitizen": "4.1.2",
    "embedme": "1.21.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^13.0.6",
    "prettier": "2.0.5",
    "protractor": "~7.0.0",
    "semantic-release": "^17.2.3",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.4.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cloudnc/ngx-observable-lifecycle.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}